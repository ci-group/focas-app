div(data-role="page", data-theme="a", id="pdf")
        include header.jade

        div(data-role="header", data-add-back-btn="true")
            span(class="ui-title")
        div(data-role="content", data-theme="a", id="pdf-embed", class="main-content")
            div
                button(id="prev") Previous
                button(id="next") Next
                &nbsp; &nbsp;
                span Page:
                    span(id="page_num") 10 /
                    span(id="page_count") 14
            canvas(id="pdf-object")

        include footer.jade
